{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "incremental": true,
    "tsBuildInfoFile": "./node_modules/.cache/vue-tsc/tsconfig.typecheck.tsbuildinfo",
    "skipLibCheck": true,
    "noEmit": true
  },
  "include": ["src/**/*.ts", "src/**/*.vue", "env.d.ts"],
  "exclude": ["node_modules", "dist", "tests", "test", "**/*.test.ts", "**/*.spec.ts"]
}
